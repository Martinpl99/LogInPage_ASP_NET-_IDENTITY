@using IDENTITY_Cwiczenia.Identity.Model.Logging;
@using IDENTITY_Cwiczenia.Controllers;
@model Login
@{
    ViewData["Title"] = "Login";
}

<h1 class="bg-info text-white">Log_in</h1>
<div class="text-danger" asp-validation-summary="All"></div>

<form asp-action="Log_in" method="post">
    <input type="hidden" asp-for="ReturnUrl" />
    <div class="form-group">
        <label asp-for="Email"></label>
        <input asp-for="Email" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Password"></label>
        <input asp-for="Password" class="form-control" />
    </div>

    <div class="form-group">
        Remember me? <input type="checkbox" asp-for="Remember" />
    </div>

    <button class="btn btn-primary" type="submit">Log In</button>
</form>
